<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="aldra-db" defaultModelType="hierarchical" targetRuntime="MyBatis3">
		<property name="javaFileEncoding" value="UTF-8" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />
		<plugin type="org.mybatis.generator.plugins.FluentBuilderMethodsPlugin" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="${driverClassName}" connectionURL="${url}" userId="${username}" password="${password}" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="true" />
			<property name="useJSR310Types" value="true" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="${targetEntityPackage}" targetProject="${targetProject}" />
		<sqlMapGenerator targetPackage="${targetMapperPackage}" targetProject="${targetProject}" />
		<javaClientGenerator type="ANNOTATEDMAPPER" targetPackage="${targetMapperPackage}" targetProject="${targetProject}" />
		<table
            tableName="sandbox"
            domainObjectName="system.gen.Sandbox"
            mapperName="system.gen.SandboxMapperDefault"
            sqlProviderName="system.gen.SandboxClockSqlProviderDefault"
            modelType="flat"
            enableSelectByExample="false"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
        />
		<table
            tableName="fixed_clock"
            domainObjectName="system.gen.FixedClock"
            mapperName="system.gen.FixedClockMapperDefault"
            sqlProviderName="system.gen.FixedClockSqlProviderDefault"
            modelType="flat"
            enableSelectByExample="false"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
        />
		<table
            tableName="staff"
            domainObjectName="user.gen.Staff"
            mapperName="user.gen.StaffMapperDefault"
            sqlProviderName="user.gen.StaffSqlProviderDefault"
            modelType="flat"
            enableSelectByExample="false"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
        />
		<table
            tableName="role"
            domainObjectName="user.gen.Role"
            mapperName="user.gen.RoleMapperDefault"
            sqlProviderName="user.gen.RoleSqlProviderDefault"
            modelType="flat"
            enableSelectByExample="false"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
        />
		<table
            tableName="authority"
            domainObjectName="user.gen.Authority"
            mapperName="user.gen.AuthorityMapperDefault"
            sqlProviderName="user.gen.AuthoritySqlProviderDefault"
            modelType="flat"
            enableSelectByExample="false"
            enableDeleteByExample="false"
            enableCountByExample="false"
            enableUpdateByExample="false"
        />
	</context>
</generatorConfiguration>
